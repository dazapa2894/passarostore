{{ 'section-lastest-products.css' | asset_url | stylesheet_tag }}

{% stylesheet %}


{% endstylesheet %}


<section id="lastest-products">

  <h2 class="title{% if section.settings.title == blank %} title--no-heading{% endif %}">{{ section.settings.title | escape }}</h2>
  

</section>

{% javascript %}

{% endjavascript %}

{% schema %}
{
  "name": "t:sections.lastest-products.name",
  "class": "lastest-products",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "t:sections.lastest-products.settings.title.label",
      "default": "Ultimas colecciones"
    },
    {
      "type": "number",
      "id": "max_products",
      "label": "Products showed",
      "default": 6
    }
  ]
}
{% endschema %}